import java.util.*;
public class removeDuplicateCharacters {
     public static void main(String args[]) {          
    	 Scanner sc=new Scanner(System.in);
    	 String s=sc.nextLine();
    	 char c[]=s.toCharArray();
    	 String t="";
    	 for(char value:c) {                       
    		 if(t.indexOf(value)==-1) {           
    		 }
    	 }
    	 System.out.println(t);
    	 
     }
}
